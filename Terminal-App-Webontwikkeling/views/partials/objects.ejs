<tr> 
  <td>
    <img
      src="<%= pokemon.imageUrl %>"
      alt="<%= pokemon.name %>"
      class="pokemon-image"
    />
  </td>
  <td>
    <a href="/pokemons/<%= pokemon.name %>"> <%= pokemon.name %> </a>
  </td>

  <td><%= pokemon.types %></td>

  <td><%= pokemon.weakness %></td>

  <td><%= pokemon.description %></td>
  <td>
    <%= pokemon.attacks.reduce((accumilator, current) =>
    `${accumilator}${accumilator ? ', ' : ''}${current.name}`, "") %>
  </td>
</tr>
